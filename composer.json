{
  "name": "hailkongsan/laravel-antibotlink",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "require": {
    "intervention/image": "^2.5",
    "illuminate/session": "^6.0 || ^7.0 || ^8.0",
    "nesbot/carbon": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Hailkongsan\\AntiBotLink\\": "src/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^4.0 || ^5.0 || 6.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Hailkongsan\\AntiBotLink\\Test\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Hailkongsan\\AntiBotLink\\AntiBotLinkServiceProvider"
      ],
      "aliases": {
        "AntiBotLink": "Hailkongsan\\AntiBotLink\\Facades\\AntiBotLink"
      }
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --testdox"
  }
}
